{
  "extends": "@nationalbankbelgium/code-style/tsconfig/3.2.x/ng7",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "dist",
    "lib": [
      "dom",
      "dom.iterable",
      "es2017"
    ],
    "paths": {
      "my-lib": [
        "dist/my-lib"
      ],
      "my-lib/*": [
        "dist/my-lib/*"
      ],
      "@nationalbankbelgium/angular-text-mask": [
        "dist/angular-text-mask"
      ],
      "@nationalbankbelgium/angular-text-mask/*": [
        "dist/angular-text-mask/*"
      ]
    }
  }
}